{
  "name": "ingest-validator",
  "version": "1.6.0",
  "description": "Ingest validator implementation in node using ajv based on https://github.com/HumanCellAtlas/ingest-validator.git which is in turn based on https://github.com/EMBL-EBI-SUBS/json-schema-validator.git.",
  "main": "dist/app.js",
  "repository": "https://github.com/ebi-ait/ingest-validator.git",
  "scripts": {
    "start": "tsc && node dist/app.js",
    "test": "jest",
    "ols": "tsc && node dist/compare_ols.js",
    "cucumber": "tsc && cucumber-js --format @cucumber/pretty-formatter",
    "test-watch": "nodemon --exec \"npm test\""
  },
  "engines": {
    "node": "8.17.0"
  },
  "keywords": [
    "nodejs",
    "json",
    "schema",
    "validator"
  ],
  "author": "Ingest dev team",
  "license": "Apache-2.0",
  "dependencies": {
    "ajv": "^6.10.2",
    "amqplib": "^0.5.5",
    "bluebird": "^3.5.5",
    "braces": ">=2.3.1",
    "config": "^2.0.2",
    "dnscache": "^1.0.2",
    "jsonwebtoken": "8.5.0",
    "lodash": ">=4.17.13",
    "mem": ">=4.0.0",
    "mixin-deep": ">=1.3.2",
    "npid": "^0.4.0",
    "ramda": "^0.26.1",
    "request": "^2.88.0",
    "request-defaults": "^1.2.0",
    "request-promise": "^4.2.5",
    "set-value": ">=2.0.1",
    "winston-daily-rotate-file": "^3.10.0",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.1.0",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.0",
    "@types/amqplib": "^0.5.13",
    "@types/bluebird": "^3.5.27",
    "@types/config": "0.0.34",
    "@types/jest": "^24.0.17",
    "@types/jsonwebtoken": "8.5.0",
    "@types/ramda": "^0.25.51",
    "@types/request": "^2.48.2",
    "@types/request-promise": "^4.1.44",
    "@types/traverson": "^2.0.28",
    "axios": "^0.19.2",
    "cryptiles": ">=4.1.2",
    "dotenv": "^16.3.1",
    "jest": "^22.4.4",
    "jest-mock": "^26.6.2",
    "merge": ">=1.2.1",
    "nodemon": "^1.19.1",
    "snyk": "^1.216.0",
    "ts-jest": "^24.0.2",
    "typemoq": "^2.1.0",
    "typescript": "^3.5.3"
  }
}
